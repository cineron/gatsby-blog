{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "__comment__": "//JSON files don't support comments or line breaks. // change develop script to use graphQL Playground // 1. $ npm install --save-dev env-cmd  //run this to install package // 2.  add .env.development file to root of project // with 'GATSBY_GRAPHQL_IDE=playground' // 3.  change: // 'develop': 'gatsby develop', // to use GraphQL Playground",
    "develop": "env-cmd .env gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.2.0",
    "gatsby": "^2.3.34",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.36",
    "gatsby-remark-images": "^3.0.11",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-contentful": "^2.0.55",
    "gatsby-source-filesystem": "^2.0.33",
    "gatsby-transformer-remark": "^2.3.12",
    "node-sass": "^7.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "env-cmd": "^8.0.2",
    "prettier": "^1.17.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
